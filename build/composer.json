{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of emmr.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/emmr.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "dompdf/dompdf": "~0.8.0",
        "drupal/bootstrap": "3.21",
        "drupal/config_update": "1.1",
        "drupal/core": "8.8.5",
        "drupal/editor_advanced_link": "1.6",
        "drupal/editor_file": "1.4",
        "drupal/entity_print": "2.x",
        "drupal/entity_reference_revisions": "1.8",
        "drupal/externalauth": "1.1",
        "drupal/features": "3.0-beta8",
        "drupal/google_analytics": "2.4",
        "drupal/health_check": "1.2",
        "drupal/ldap": "3.0-beta3",
        "drupal/override_node_options": "2.x-dev",
        "drupal/paragraphs": "1.11",
        "drupal/pathauto": "1.7",
        "drupal/redis": "1.4",
        "drupal/search_api": "1.16",
        "drupal/search_api_autocomplete": "1.2",
        "drupal/search_api_solr": "3.9",
        "drupal/token_filter": "1.0",
        "drupal/twig_tweak": "2.5",
        "drupal/view_unpublished": "1.0.0-alpha1",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "tecnickcom/tcpdf": "~6",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-3.x",
        "unb-libraries/drupal-builder": "^8.0.0"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
