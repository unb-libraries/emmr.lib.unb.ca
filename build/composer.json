{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of emmr.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/emmr.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "dompdf/dompdf": "~0.8.0",
        "drupal/bootstrap": "3.23",
        "drupal/core": "9.2.4",
        "drupal/editor_advanced_link": "1.9",
        "drupal/editor_file": "1.5",
        "drupal/entity_reference_revisions": "1.9",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.3",
        "drupal/override_node_options": "2.x-dev",
        "drupal/paragraphs": "1.12",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.5",
        "drupal/search_api": "1.20",
        "drupal/search_api_solr": "4.2.1",
        "drupal/twig_tweak": "2.9",
        "drupal/view_unpublished": "1.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "tecnickcom/tcpdf": "~6",
        "unb-libraries/unb_lib_theme": "dev-9.x-bootstrap-3.x",
        "unb-libraries/drupal-builder": "^9"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^9"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
