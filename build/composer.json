{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of emmr.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/emmr.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "dompdf/dompdf": "^2",
    "drupal/bootstrap": "^3",
    "drupal/ckeditor5_allowed_html": "1.1.x-dev",
    "drupal/color": "^1",
    "drupal/core": "^10",    
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/editor_advanced_link": "^2",
    "drupal/entity_reference_revisions": "^1",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/override_node_options": "^2",
    "drupal/paragraphs": "^1",
    "drupal/pathauto": "^1",
    "drupal/quickedit": "^1",
    "drupal/redis": "^1",
    "drupal/search_api": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/simple_sitemap": "4.1.8",
    "drupal/twig_tweak": "^3",
    "drupal/view_unpublished": "^1",
    "mikehaertl/phpwkhtmltopdf": "^2",
    "tecnickcom/tcpdf": "^6",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
